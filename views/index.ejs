<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/css/style.css">
    <title>Document</title>
</head>
<body>
    <main>
        <h1>Welcome</h1>
        <h3>Categories</h3>
        <ul>
            <% puzzle.categories.forEach((category) => { %>
                <li><%= category %></li>
            <% }) %>
        </ul>
        <h3>Clues</h3>
        <ul>
            <% puzzle.clues.forEach((clue) => { %>
                <li><%= clue %></li>
            <% }) %>
        </ul>
    </main>
</body>
</html>